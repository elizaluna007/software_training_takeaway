	margin: 30rpx;
	border-width: 100%;
	border: 3rpx solid #f8f8f8;
	box-shadow: #8f8f94;
	border-radius: 7%;
	box-shadow:
	  5.7px 3.8px 5.3px rgba(0, 0, 0, 0.04),
